{
  "name": "vite-js-react-minimal",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite ",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && npx vercel ./dist --prod",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore src ",
    "lint:fix": "eslint --ext .js,.jsx --ignore-path .gitignore --fix src",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:coverage:all": "jest --coverage --collectCoverageFrom='./src/**'",
    "test:e2e": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^29.5.0",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "redux-mock-store": "^1.5.4",
    "vite": "^4.2.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
